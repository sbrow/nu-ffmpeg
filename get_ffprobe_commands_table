#!/usr/bin/env -S nu -n
# vim: filetype=nu :

use ffprobe;

def main [] {
  (
    help commands
    | where command_type == "custom" and name !~ "p(rompt|wd)"
    | select name description
    | update name { if $in == 'main' { 'ffprobe' } else { $'ffprobe ($in)' } }
    | where { not ($in.name == 'ffprobe' and ($in.description | is-empty)) }
    | to md -p
  )
}
